{
  "name": "aui-compass",
  "version": "1.0.0",
  "description": "Figma plugin for migrating deprecated AUI design system components",
  "main": "code.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "watch": "tsc -p tsconfig.json --watch",
    "get-team-id": "ts-node src/scripts/getTeamId.ts",
    "get-library-files": "ts-node src/scripts/getLibraryFiles.ts",
    "get-component-details": "ts-node src/scripts/getComponentDetails.ts",
    "fetch-component-details": "ts-node src/scripts/fetchComponentDetails.ts",
    "update-registry": "ts-node src/scripts/updateRegistry.ts",
    "map-components": "ts-node src/scripts/mapComponents.ts",
    "validate-mappings": "ts-node src/scripts/validateMappings.ts",
    "generate-registry": "ts-node src/scripts/generateRegistry.ts"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@figma/plugin-typings": "^1.82.0",
    "@types/node": "^20.11.19",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "axios": "^1.6.7"
  }
}
